<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="usuarioWebFB" type="com.immobilis.usuarioweb.form.UsuarioWebForm" />
	</form-beans>
	
	<global-forwards></global-forwards>
	
	<action-mappings>
		<action
            name ="usuarioWebFB"
            path="/usuarioWebAction"
            type="com.immobilis.usuarioweb.action.UsuarioWebAction"
            parameter="method"
            input="/pages/index.jsp"
            scope="request">
            <forward name="index"        path="/pages/home.jsp"/>     
            <forward name="usuarioWeb"        path="/pages/usuarioWeb.jsp"/>
            
        </action>
        
	</action-mappings>
	
	
</struts-config>